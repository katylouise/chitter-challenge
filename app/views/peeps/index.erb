<h1>Peeps</h1>

<div id='peeps'>
  <% @peeps.each do |peep| %>
    <div class='peep'>
      <%= peep.content %>
      <ul class='peep_info'>
        Created by: <li><%= peep.user.name %></li>
        Logged in as: <li><%= peep.user.username %></li>
        Created at: <li><%= peep.time_created %></li>
      </ul>
    </div>
  <% end %>
</div>